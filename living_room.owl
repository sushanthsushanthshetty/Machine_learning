<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/house_ontology.owl"
         xmlns="http://example.org/house_ontology.owl#">

<owl:Ontology rdf:about="http://example.org/house_ontology.owl"/>

<owl:ObjectProperty rdf:about="#hasFurniture">
  <rdfs:domain rdf:resource="#Room"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDevice">
  <rdfs:domain rdf:resource="#Room"/>
  <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#Room">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#LivingRoom">
  <rdfs:subClassOf rdf:resource="#Room"/>
</owl:Class>

<owl:Class rdf:about="#Sofa">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Telivision">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#MyLivingRoom">
  <rdf:type rdf:resource="#LivingRoom"/>
  <hasFurniture rdf:resource="#MySofa"/>
  <hasDevice rdf:resource="#MyTV"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#MySofa">
  <rdf:type rdf:resource="#Sofa"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#MyTV">
  <rdf:type rdf:resource="#Telivision"/>
</owl:NamedIndividual>


</rdf:RDF>
